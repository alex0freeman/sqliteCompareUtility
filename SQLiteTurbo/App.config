<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net"/>
  </configSections>
  <log4net>
    <appender name="LocalUserFileAppender" type="SQLiteTurbo.LocalUserFileAppender">
    </appender>
    <root>
      <level value="Debug"/>
      <appender-ref ref="LocalUserFileAppender"/>
    </root>
  </log4net>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0,Profile=Client"/></startup></configuration>
